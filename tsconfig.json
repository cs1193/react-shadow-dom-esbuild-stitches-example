{
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "downlevelIteration": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "forceConsistentCasingInFileNames": true
  }
}
